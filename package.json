{
  "name": "another-ui",
  "private": true,
  "version": "0.1.0",
  "description": "Just another library for React components",
  "main": "index.js",
  "scripts": {
    "build": "lerna run build",
    "build-storybook": "rimraf docs && build-storybook -o docs",
    "clean": "lerna run clean",
    "lerna:bootstrap": "lerna bootstrap",
    "lerna:clean": "lerna clean",
    "lint": "eslint . --ext .js,.ts,.tsx && sass-lint -v",
    "postinstall": "lerna bootstrap",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nghuudan/another-ui.git"
  },
  "keywords": [
    "another",
    "ui",
    "component"
  ],
  "author": "Dan Nguyen <nghuudan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nghuudan/another-ui/issues"
  },
  "homepage": "https://github.com/nghuudan/another-ui#readme",
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@fontsource/open-sans": "^4.2.2",
    "@octokit/core": "^3.2.5",
    "@storybook/addon-a11y": "^6.1.21",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/cli": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.1.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "postcss": "^8.2.7",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "sass-lint": "^1.13.1",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3",
    "webpack": "^4.46.0"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
